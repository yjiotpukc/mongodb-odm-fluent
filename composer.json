{
  "name": "yjiotpukc/mongodb-odm-fluent",
  "description": "A fluent PHP mapping driver for Doctrine MongoDB ODM.",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "laravel",
    "mongodb",
    "odm",
    "data mapper",
    "database",
    "mappings",
    "mapping driver",
    "fluent"
  ],
  "authors": [
    {
      "name": "Vadim Zubko",
      "email": "yjiotpukc@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "doctrine/mongodb-odm": "^2.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "yjiotpukc\\MongoODMFluent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yjiotpukc\\MongoODMFluent\\Tests\\": "tests/",
      "yjiotpukc\\MongoODMFluent\\Examples\\": "examples/"
    }
  },
  "scripts": {
    "test-unit": "vendor/bin/phpunit --testsuite unit",
    "test-integration-mapping": "vendor/bin/phpunit --testsuite integration-mapping",
    "test-full": [
      "@test-unit",
      "@test-integration-mapping"
    ],
    "coverage-unit": "XDEBUG_MODE=coverage vendor/bin/phpunit --testsuite unit --coverage-clover unit-coverage.xml --coverage-filter src",
    "coverage-integration-mapping": "XDEBUG_MODE=coverage vendor/bin/phpunit --testsuite integration-mapping --coverage-clover integration-mapping-coverage.xml --coverage-filter src",
    "coverage-full": [
      "@coverage-unit",
      "@coverage-integration-mapping"
    ]
  }
}
